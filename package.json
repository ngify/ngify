{
	"name": "ngify",
	"version": "1.3.0",
	"description": "Pretend to be using Angular",
	"main": "index.js",
	"scripts": {
		"dev:store": "node scripts/build.mjs --pkg=store",
		"dev:http": "node scripts/build.mjs --pkg=http",
		"dev:at": "node scripts/build.mjs --pkg=at",
		"dev:types": "node scripts/build.mjs --pkg=types",
		"dev:docs": "typedoc --watch",
		"build:store": "node scripts/build.mjs --pkg=store --prod",
		"build:http": "node scripts/build.mjs --pkg=http --prod",
		"build:at": "node scripts/build.mjs --pkg=at --prod",
		"build:types": "node scripts/build.mjs --pkg=types --prod",
		"build": "node scripts/build.mjs --prod",
		"build:docs": "typedoc",
		"test": "lerna run --scope @ngify/http --scope @ngify/store --scope @ngify/at test:ci",
		"publish": "npm run build & lerna publish --force-publish=* --exact",
		"version": "conventional-changelog -p angular -i CHANGELOG.md -s && git add CHANGELOG.md",
		"bootstrap": "lerna bootstrap --hoist",
		"clean": "lerna clean"
	},
	"author": "HyperLife1119 <hyperlife1119@qq.com>",
	"license": "MIT",
	"private": true,
	"workspaces": [
		"packages/*"
	],
	"devDependencies": {
		"@rollup/plugin-node-resolve": "14.1.0",
		"@rollup/plugin-typescript": "8.5.0",
		"conventional-changelog-cli": "2.2.2",
		"lerna": "5.5.0",
		"minimist": "1.2.6",
		"rollup": "2.79.0",
		"typedoc": "0.23.15",
		"typescript": "4.6.3"
	},
	"dependencies": {
		"tslib": "2.4.0"
	}
}