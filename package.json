{
	"name": "ngify",
	"version": "1.0.0",
	"description": "Pretend to be using Angular",
	"main": "index.js",
	"scripts": {
		"dev:store": "node scripts/build.js --pkg=store",
		"dev:http": "node scripts/build.js --pkg=http",
		"dev:types": "node scripts/build.js --pkg=types",
		"build:store": "node scripts/build.js --pkg=store --prod",
		"build:http": "node scripts/build.js --pkg=http --prod",
		"build:types": "node scripts/build.js --pkg=types --prod",
		"build": "node scripts/build.js --prod",
		"publish": "npm run build & lerna publish --force-publish=* --exact",
		"bootstrap": "lerna bootstrap --hoist"
	},
	"author": "HyperLife1119 <hyperlife1119@qq.com>",
	"license": "MIT",
	"workspaces": [
		"packages/*"
	],
	"devDependencies": {
		"@rollup/plugin-node-resolve": "^13.0.4",
		"@rollup/plugin-typescript": "^8.2.5",
		"lerna": "^4.0.0",
		"minimist": "^1.2.5",
		"rollup": "^2.56.3",
		"rollup-plugin-terser": "^7.0.2",
		"typescript": "^4.3.5"
	},
	"dependencies": {
		"tslib": "^2.3.1"
	}
}