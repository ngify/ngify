{
  "name": "@ngify/store",
  "version": "1.5.3",
  "description": "A simple state manager based on decorators and rxjs.",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "files": [
    "src",
    "dist"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    },
    "./package.json": "./package.json"
  },
  "sideEffects": false,
  "scripts": {
    "dev": "rollup --config rollup.config.mjs --watch",
    "build": "rollup --config rollup.config.mjs",
    "test": "jest --config ./jest.config.json",
    "test:ci": "jest --config ./jest.config.json -i",
    "test:coverage": "jest --config ./jest.config.json --coverage",
    "lint": "eslint . --fix"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ngify/ngify.git"
  },
  "author": "HyperLife1119 <hyperlife1119@qq.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ngify/ngify/issues"
  },
  "homepage": "https://github.com/ngify/ngify/tree/main/packages/store",
  "keywords": [
    "state",
    "state management",
    "decorator",
    "observable",
    "reactive",
    "rxjs"
  ],
  "publishConfig": {
    "access": "public"
  },
  "dependencies": {
    "@ngify/types": "1.5.3"
  },
  "peerDependencies": {
    "rxjs": "^7.0.0"
  }
}
