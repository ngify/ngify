{
  "name": "@ngify/http",
  "version": "1.5.3",
  "description": "A reactive HTTP client API",
  "main": "./dist/cjs/index.js",
  "module": "./dist/esm/index.js",
  "types": "./dist/types/index.d.ts",
  "files": [
    "src",
    "dist",
    "wx/src",
    "wx/dist",
    "wx/package.json",
    "fetch/src",
    "fetch/dist",
    "fetch/package.json",
    "testing/src",
    "testing/dist",
    "testing/package.json"
  ],
  "sideEffects": false,
  "scripts": {
    "dev": "node ../../scripts/build.mjs --pkg=http",
    "build": "node ../../scripts/build.mjs --pkg=http --prod",
    "build:wx": "node ../../scripts/build.mjs --pkg=http/wx --prod",
    "build:fetch": "node ../../scripts/build.mjs --pkg=http/fetch --prod",
    "build:testing": "node ../../scripts/build.mjs --pkg=http/testing --prod",
    "postbuild": "npm run build:wx & npm run build:fetch & npm run build:testing",
    "test": "jest --config ./jest.config.json",
    "test:ci": "jest --config ./jest.config.json -i",
    "test:coverage": "jest --config ./jest.config.json --coverage",
    "lint": "eslint . --fix"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ngify/ngify.git"
  },
  "author": "HyperLife1119 <hyperlife1119@qq.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ngify/ngify/issues"
  },
  "homepage": "https://github.com/ngify/ngify/tree/main/packages/http",
  "keywords": [
    "wx",
    "xhr",
    "fetch",
    "http",
    "ajax",
    "interceptor",
    "observable",
    "reactive",
    "rxjs",
    "angular"
  ],
  "publishConfig": {
    "access": "public"
  },
  "dependencies": {
    "@ngify/types": "1.5.3",
    "miniprogram-api-typings": "3.10.0",
    "tslib": "^2.0.0"
  },
  "peerDependencies": {
    "miniprogram-api-typings": "3.10.0",
    "rxjs": "^7.0.0"
  }
}
