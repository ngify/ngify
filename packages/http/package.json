{
  "name": "@ngify/http",
  "version": "1.5.3",
  "description": "A reactive HTTP client API",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "files": [
    "src",
    "dist",
    "wx/src",
    "wx/package.json",
    "fetch/src",
    "fetch/package.json",
    "testing/src",
    "testing/package.json"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    },
    "./wx": {
      "types": "./dist/wx/index.d.ts",
      "import": "./dist/wx/index.mjs",
      "require": "./dist/wx/index.js"
    },
    "./fetch": {
      "types": "./dist/fetch/index.d.ts",
      "import": "./dist/fetch/index.mjs",
      "require": "./dist/fetch/index.js"
    },
    "./testing": {
      "types": "./dist/testing/index.d.ts",
      "import": "./dist/testing/index.mjs",
      "require": "./dist/testing/index.js"
    },
    "./package.json": "./package.json"
  },
  "sideEffects": false,
  "scripts": {
    "dev": "rollup --config rollup.config.mjs --watch",
    "build": "rollup --config rollup.config.mjs",
    "test": "jest --config ./jest.config.json",
    "test:ci": "jest --config ./jest.config.json -i",
    "test:coverage": "jest --config ./jest.config.json --coverage",
    "lint": "eslint . --fix"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ngify/ngify.git"
  },
  "author": "HyperLife1119 <hyperlife1119@qq.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ngify/ngify/issues"
  },
  "homepage": "https://github.com/ngify/ngify/tree/main/packages/http",
  "keywords": [
    "ajax",
    "wx",
    "miniprogram",
    "http",
    "http-client",
    "observable",
    "observable-http",
    "reactive",
    "reactive-http",
    "rxjs",
    "rxjs-http",
    "angular",
    "angular-http"
  ],
  "publishConfig": {
    "access": "public"
  },
  "dependencies": {
    "@ngify/types": "1.5.3",
    "miniprogram-api-typings": "3.10.0",
    "tslib": "^2.0.0"
  },
  "peerDependencies": {
    "miniprogram-api-typings": "3.10.0",
    "rxjs": "^7.0.0"
  }
}
