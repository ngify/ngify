{
  "displayName": "at",
  "preset": "ts-jest/presets/default-esm",
  "rootDir": "./",
  "testMatch": [
    "<rootDir>/test/*.spec.ts"
  ],
  "coverageDirectory": "./coverage",
  "transform": {
    ".ts": [
      "ts-jest",
      {
        "isolatedModules": true
      }
    ]
  },
  "moduleNameMapper": {
    "^lodash-es$": "lodash"
  }
}
