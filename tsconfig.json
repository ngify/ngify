{
  "exclude": [
    "dist",
  ],
  "compilerOptions": {
    "strict": true,
    "target": "es2015",
    "module": "esnext",
    "moduleResolution": "node",
    "baseUrl": ".",
    "importHelpers": true,
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "skipLibCheck": true,
    "preserveConstEnums": true,
  }
}
